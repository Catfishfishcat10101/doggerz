{
  "name": "doggerz",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host 127.0.0.1 --port 5173",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "build": "vite build",
    "preflight": "node scripts/preflight.js",
    "env:check": "node scripts/check-env.js",
    "ci:verify": "npm run lint && npm run build && npm run preflight",
    "ci:playstore": "npm run env:check -- --strict --require firebase && npm run preflight -- --strict --require assetlinks && npm run build",
    "assets:report": "node scripts/report-assets.js",
    "assets:unused": "node scripts/find-unused-public-assets.js",
    "assets:missing": "node scripts/check-missing-public-assets.js",
    "assets:prune:binaries": "node scripts/prune-public-binaries.js",
    "assets:prune:binaries:yes": "node scripts/prune-public-binaries.js --yes",
    "icons:generate": "node scripts/generate-app-icons.js",
    "images:optimize": "node scripts/optimize-images.js",
    "audio:optimize": "node scripts/optimize-audio.js",
    "clean:generated": "node scripts/clean-generated-assets.js",
    "sprites:webp": "node scripts/make-spritesheet-webp.js",
    "sprites:webp:dry": "node scripts/make-spritesheet-webp.js --dryRun",
    "sprites:jrt": "node scripts/generate-jrt-sprite-pack.js",
    "sprites:jrt:scaffold": "node scripts/scaffold-jrt-frames.js",
    "sprites:jrt:validate": "node scripts/validate-sprite-frames.js",
    "sprites:jrt:build": "node scripts/build-sprite-strips-from-frames.js",
    "sprites:jrt:static": "node scripts/generate-jrt-static-sprites.js",
    "sprites:jrt:mock-frames": "node scripts/generate-mock-jrt-frames.js",
    "sprites:jrt:mock-core": "node scripts/generate-mock-jrt-frames.js --group core --force",
    "sprites:jrt:mock-all": "node scripts/generate-mock-jrt-frames.js --group all --force",
    "sprites:jrt:checklist": "node scripts/generate-sprite-asset-checklist.js",
    "sprites:jrt:reset": "node scripts/reset-jrt-sprites.js",
    "sprites:jrt:base": "node scripts/generate-jack-russell-base-sprites.js",
    "sprites:jrt:regen": "npm run sprites:jrt:reset && npm run sprites:jrt:base && npm run sprites:jrt:mock-all && npm run sprites:jrt:build && npm run sprites:jrt:static",
    "sprites:jrt:prune": "node scripts/prune-jrt-public-sprites.js",
    "sprites:jrt:prune:yes": "node scripts/prune-jrt-public-sprites.js --yes",
    "repo:prune": "node scripts/prune-redundant-files.js",
    "repo:prune:yes": "node scripts/prune-redundant-files.js --yes",
    "sw:bump": "node scripts/bump-sw-cache-version.js",
    "bundle:analyze": "node scripts/analyze-bundle.js",
    "preview": "vite preview",
    "preview:host": "vite preview --host 127.0.0.1 --port 4173",
    "split:backyard": "node scripts/split-backyard.js public/backgrounds/backyard-split.png public/backgrounds/backyard-day.webp public/backgrounds/backyard-night.webp"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@pixi/react": "^7.1.2",
    "@reduxjs/toolkit": "^2.10.1",
    "@vercel/analytics": "^1.6.1",
    "firebase": "^11.10.0",
    "lottie-react": "^2.4.1",
    "pixi.js": "^8.14.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/react": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-formatter-unix": "^9.0.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "ffmpeg-static": "^5.2.0",
    "globals": "^15.14.0",
    "postcss": "^8.4.47",
    "rollup": "^4.53.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.13",
    "vite": "^7.3.0"
  }
}
