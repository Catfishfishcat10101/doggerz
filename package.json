{
  "name": "doggerz",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=18.18"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --strictPort --port 5173",
    "clean": "rimraf dist .vite",
    "format": "prettier --write .",
    "lint": "eslint .",
    "emulators": "firebase emulators:start --only auth,firestore,ui",
    "emudev": "concurrently \"npm:emulators\" \"npm:dev\"",
    "analyze": "vite build && npx rollup-plugin-visualizer dist/stats.html && node -e \"console.log('Analyze: open dist/stats.html')\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{css,md,json}": "prettier --write"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@reduxjs/toolkit": "^2.9.0",
    "ci": "^2.3.0",
    "clsx": "^2.1.1",
    "firebase": "^11.10.0",
    "framer-motion": "^12.23.21",
    "howler": "^2.2.4",
    "lucide-react": "^0.544.0",
    "nanoid": "^5.1.6",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "6.26.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "eslint": "^9.12.0",
    "firebase-tools": "^14.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-preset-env": "^10.4.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.18",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.1.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "packageManager": "npm@10"
}
